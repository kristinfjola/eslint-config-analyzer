{
  "name": "test",

  "author": "test",

  "devDependencies": {
    "ava": "~0.16",
    "eslint": "^3",
    "eslint-config-brunch": "^1",
  },

  // comment
  "eslintConfig": { // rules
    "globals": {
      "window": true,
      "console": true
    },
    // comment
    "rules": {  // comment
      "block-scoped-var": 2,  // comment
      "comma-dangle": "off",
      "quotes": [
        1,  // comment
        "double", // comment
        "avoid-escape"
      ]
    }
  },

  "scripts": {
    "script1": "script",
    "script2": "script",
  },

  "babel": {
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  }
}